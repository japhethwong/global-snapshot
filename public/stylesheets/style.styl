html
  height: 100%
body
  height: 100%
  width: 100%
  background: white
  font: 14px "century gothic", tahoma, "Lucida Grande", Helvetica, Arial, sans-serif
  margin: 0
a
  color: #00B7FF
#name
  font-style: italic
#header
  height: 50px
  width: 100%
  background-color: white
#map
  width: 100%
  height: 100%
  position: absolute
  float: left
#feed
  width: 200px
  height: 100%
  float: right
  background-opacity: 0.8
  background-color: white
  z-index: 1
  overflow-y: hidden
#feed img
  width: 100%